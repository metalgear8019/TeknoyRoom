                               // 74\n        this.$widget = false;                                                                                         // 75\n      }                                                                                                               // 76\n                                                                                                                      // 77\n      if (this.showInputs && this.$widget !== false) {                                                                // 78\n        this.$widget.find('input').each(function() {                                                                  // 79\n          $(this).on({                                                                                                // 80\n            'click.timepicker': function() { $(this).select(); },                                                     // 81\n            'keydown.timepicker': $.proxy(self.widgetKeydown, self),                                                  // 82\n            'keyup.timepicker': $.proxy(self.widgetKeyup, self)                                                       // 83\n          });                                                                                                         // 84\n        });                                                                                                           // 85\n      }                                                                                                               // 86\n                                                                                                                      // 87\n      this.setDefaultTime(this.defaultTime);                                                                          // 88\n    },                                                                                                                // 89\n                                                                                                                      // 90\n    blurElement: function() {                                                                                         // 91\n      this.highlightedUnit = null;                                                                                    // 92\n      this.updateFromElementVal();                                                                                    // 93\n    },                                                                                                                // 94\n                                                                                                                      // 95\n    clear: function() {                                                                                               // 96\n      this.hour = '';                                                                                                 // 97\n      this.minute = '';                                                                                               // 98\n      this.second = '';                                                                                               // 99\n      this.meridian = '';                                                                                             // 100\n                                                                                                                      // 101\n      this.$element.val('');                                                                                          // 102\n    },                                                                                                                // 103\n                                                                                                                      // 104\n    decrementHour: function() {                                                                                       // 105\n      if (this.showMeridian) {                                                                                        // 106\n        if (this.hour === 1) {                                                                                        // 107\n          this.hour = 12;                                                                                             // 108\n        } else if (this.hour === 12) {                                                                                // 109\n          this.hour--;                                                                                                // 110\n                                                                                                                      // 111\n          return this.toggleMeridian();                                                                               // 112\n        } else if (this.hour === 0) {                                                                                 // 113\n          this.hour = 11;                                                                                             // 114\n                                                                                                                      // 115\n          return this.toggleMeridian();                                                                               // 116\n        } else {                                                                                                      // 117\n          this.hour--;                                                                                                // 118\n        }                                                                                                             // 119\n      } else {                                                                                                        // 120\n        if (this.hour <= 0) {                                                                                         // 121\n          this.hour = 23;                                                                                             // 122\n        } else {                                                                                                      // 123\n          this.hour--;                                                                                                // 124\n        }                                                                                                             // 125\n      }                                                                                                               // 126\n    },                                                                                                                // 127\n                                                                                                                      // 128\n    decrementMinute: function(step) {                                                                                 // 129\n      var newVal;                                                                                                     // 130\n                                                                                                                      // 131\n      if (step) {                                                                                                     // 132\n        newVal = this.minute - step;                                                                                  // 133\n      } else {                                                                                                        // 134\n        newVal = this.minute - this.minuteStep;                                                                       // 135\n      }                                                                                                               // 136\n                                                                                                                      // 137\n      if (newVal < 0) {                                                                                               // 138\n        this.decrementHour();                                                                                         // 139\n        this.minute = newVal + 60;                                                                                    // 140\n      } else {                                                                                                        // 141\n        this.minute = newVal;                                                                                         // 142\n      }                                                                                                               // 143\n    },                                                                                                                // 144\n                                                                                                                      // 145\n    decrementSecond: function() {                                                                                     // 146\n      var newVal = this.second - this.secondStep;                                                                     // 147\n                                                                                                                      // 148\n      if (newVal < 0) {                                                                                               // 149\n        this.decrementMinute(true);                                                                                   // 150\n        this.second = newVal + 60;                                                                                    // 151\n      } else {                                                                                                        // 152\n        this.second = newVal;                                                                                         // 153\n      }                                                                                                               // 154\n    },                                                                                                                // 155\n                                                                                                                      // 156\n    elementKeydown: function(e) {                                                                                     // 157\n      switch (e.keyCode) {                                                                                            // 158\n      case 9: //tab                                                                                                   // 159\n      case 27: // escape                                                                                              // 160\n        this.updateFromElementVal();                                                                                  // 161\n        break;                                                                                                        // 162\n      case 37: // left arrow                                                                                          // 163\n        e.preventDefault();                                                                                           // 164\n        this.highlightPrevUnit();                                                                                     // 165\n        break;                                                                                                        // 166\n      case 38: // up arrow                                                                                            // 167\n        e.preventDefault();                                                                                           // 168\n        switch (this.highlightedUnit) {                                                                               // 169\n        case 'hour':                                                                                                  // 170\n          this.incrementHour();                                                                                       // 171\n          this.highlightHour();                                                                                       // 172\n          break;                                                                                                      // 173\n        case 'minute':                                                                                                // 174\n          this.incrementMinute();                                                                                     // 175\n          this.highlightMinute();                                                                                     // 176\n          break;                                                                                                      // 177\n        case 'second':                                                                                                // 178\n          this.incrementSecond();                                                                                     // 179\n          this.highlightSecond();                                                                                     // 180\n          break;                                                                                                      // 181\n        case 'meridian':                                                                                              // 182\n          this.toggleMeridian();                                                                                      // 183\n          this.highlightMeridian();                                                                                   // 184\n          break;                                                                                                      // 185\n        }                                                                                                             // 186\n        this.update();                                                                                                // 187\n        break;                                                                                                        // 188\n      case 39: // right arrow                                                                                         // 189\n        e.preventDefault();                                                                                           // 190\n        this.highlightNextUnit();                                                                                     // 191\n        break;                                                                                                        // 192\n      case 40: // down arrow                                                                                          // 193\n        e.preventDefault();                                                                                           // 194\n        switch (this.highlightedUnit) {                                                                               // 195\n        case 'hour':                                                                                                  // 196\n          this.decrementHour();                                                                                       // 197\n          this.highlightHour();                                                                                       // 198\n          break;                                                                                                      // 199\n        case 'minute':                                                                                                // 200\n          this.decrementMinute();                                                                                     // 201\n          this.highlightMinute();                                                                                     // 202\n          break;                                                                                                      // 203\n        case 'second':                                                                                                // 204\n          this.decrementSecond();                                                                                     // 205\n          this.highlightSecond();                                                                                     // 206\n          break;                                                                                                      // 207\n        case 'meridian':                                                                                              // 208\n          this.toggleMeridian();                                                                                      // 209\n          this.highlightMeridian();                                                                                   // 210\n          break;                                                                                                      // 211\n        }                                                                                                             // 212\n                                                                                                                      // 213\n        this.update();                                                                                                // 214\n        break;                                                                                                        // 215\n      }                                                                                                               // 216\n    },                                                                                                                // 217\n                                                                                                                      // 218\n    getCursorPosition: function() {                                                                                   // 219\n      var input = this.$element.get(0);                                                                               // 220\n                                                                                                                      // 221\n      if ('selectionStart' in input) {// Standard-compliant browsers                                                  // 222\n                                                                                                                      // 223\n        return input.selectionStart;                                                                                  // 224\n      } else if (document.selection) {// IE fix                                                                       // 225\n        input.focus();                                                                                                // 226\n        var sel = document.selection.createRange(),                                                                   // 227\n          selLen = document.selection.createRange().text.length;                                                      // 228\n                                                                                                                      // 229\n        sel.moveStart('character', - input.value.length);                                                             // 230\n                                                                                                                      // 231\n        return sel.text.length - selLen;                                                                              // 232\n      }                                                                                                               // 233\n    },                                                                                                                // 234\n                                                                                                                      // 235\n    getTemplate: function() {                                                                                         // 236\n      var template,                                                                                                   // 237\n        hourTemplate,                                                                                                 // 238\n        minuteTemplate,                                                                                               // 239\n        secondTemplate,                                                                                               // 240\n        meridianTemplate,                                                                                             // 241\n        templateContent;                                                                                              // 242\n                                                                                                                      // 243\n      if (this.showInputs) {                                                                                          // 244\n        hourTemplate = '<input type=\"text\" data-name=\"hour\" class=\"bootstrap-timepicker-hour form-control\" maxlength=\"2\"/>';\n        minuteTemplate = '<input type=\"text\" data-name=\"minute\" class=\"bootstrap-timepicker-minute form-control\" maxlength=\"2\"/>';\n        secondTemplate = '<input type=\"text\" data-name=\"second\" class=\"bootstrap-timepicker-second form-control\" maxlength=\"2\"/>';\n        meridianTemplate = '<input type=\"text\" data-name=\"meridian\" class=\"bootstrap-timepicker-meridian form-control\" maxlength=\"2\"/>';\n      } else {                                                                                                        // 249\n        hourTemplate = '<span class=\"bootstrap-timepicker-hour\"></span>';                                             // 250\n        minuteTemplate = '<span class=\"bootstrap-timepicker-minute\"></span>';                                         // 251\n        secondTemplate = '<span class=\"bootstrap-timepicker-second\"></span>';                                         // 252\n        meridianTemplate = '<span class=\"bootstrap-timepicker-meridian\"></span>';                                     // 253\n      }                                                                                                               // 254\n                                                                                                                      // 255\n      templateContent = '<table>'+                                                                                    // 256\n         '<tr>'+                                                                                                      // 257\n           '<td><a href=\"#\" data-action=\"incrementHour\"><i class=\"glyphicon glyphicon-chevron-up\"></i></a></td>'+     // 258\n           '<td class=\"separator\">&nbsp;</td>'+                                                                       // 259\n           '<td><a href=\"#\" data-action=\"incrementMinute\"><i class=\"glyphicon glyphicon-chevron-up\"></i></a></td>'+   // 260\n           (this.showSeconds ?                                                                                        // 261\n             '<td class=\"separator\">&nbsp;</td>'+                                                                     // 262\n             '<td><a href=\"#\" data-action=\"incrementSecond\"><i class=\"glyphicon glyphicon-chevron-up\"></i></a></td>'  // 263\n           : '') +                                                                                                    // 264\n           (this.showMeridian ?                                                                                       // 265\n             '<td class=\"separator\">&nbsp;</td>'+                                                                     // 266\n             '<td class=\"meridian-column\"><a href=\"#\" data-action=\"toggleMeridian\"><i class=\"glyphicon glyphicon-chevron-up\"></i></a></td>'\n           : '') +                                                                                                    // 268\n         '</tr>'+                                                                                                     // 269\n         '<tr>'+                                                                                                      // 270\n           '<td><span>'+ hourTemplate +'</span></td> '+                                                               // 271\n           '<td class=\"separator\">:</td>'+                                                                            // 272\n           '<td><span>'+ minuteTemplate +'</span></td> '+                                                             // 273\n           (this.showSeconds ?                                                                                        // 274\n            '<td class=\"separator\">:</td>'+                                                                           // 275\n            '<td><span>'+ secondTemplate +'</span></td>'                                                              // 276\n           : '') +                                                                                                    // 277\n           (this.showMeridian ?                                                                                       // 278\n            '<td class=\"separator\">&nbsp;</td>'+                                                                      // 279\n            '<td><span>'+ meridianTemplate +'</span></td>'                                                            // 280\n           : '') +                                                                                                    // 281\n         '</tr>'+                                                                                                     // 282\n         '<tr>'+                                                                                                      // 283\n           '<td><a href=\"#\" data-action=\"decrementHour\"><i class=\"glyphicon glyphicon-chevron-down\"></i></a></td>'+   // 284\n           '<td class=\"separator\"></td>'+                                                                             // 285\n           '<td><a href=\"#\" data-action=\"decrementMinute\"><i class=\"glyphicon glyphicon-chevron-down\"></i></a></td>'+ // 286\n           (this.showSeconds ?                                                                                        // 287\n            '<td class=\"separator\">&nbsp;</td>'+                                                                      // 288\n            '<td><a href=\"#\" data-action=\"decrementSecond\"><i class=\"glyphicon glyphicon-chevron-down\"></i></a></td>' // 289\n           : '') +                                                                                                    // 290\n           (this.showMeridian ?                                                                                       // 291\n            '<td class=\"separator\">&nbsp;</td>'+                                                                      // 292\n            '<td><a href=\"#\" data-action=\"toggleMeridian\"><i class=\"glyphicon glyphicon-chevron-down\"></i></a></td>'  // 293\n           : '') +                                                                                                    // 294\n         '</tr>'+                                                                                                     // 295\n       '</table>';                                                                                                    // 296\n                                                                                                                      // 297\n      switch(this.template) {                                                                                         // 298\n      case 'modal':                                                                                                   // 299\n        template = '<div class=\"bootstrap-timepicker-widget modal fade\" data-backdrop=\"'+ (this.modalBackdrop ? 'true' : 'false') +'\">'+\n          '<div class=\"modal-dialog\">' +                                                                              // 301\n            '<div class=\"modal-content\">' +                                                                           // 302\n              '<div class=\"modal-header\">'+                                                                           // 303\n                '<a href=\"#\" class=\"close\" data-dismiss=\"modal\">Ã—</a>'+                                               // 304\n                '<h3>Pick a Time</h3>'+                                                                               // 305\n              '</div>'+                                                                                               // 306\n              '<div class=\"modal-body\">'+                                                                             // 307\n                templateContent +                                                                                     // 308\n              '</div>'+                                                                                               // 309\n              '<div class=\"modal-footer\">'+                                                                           // 310\n                '<a href=\"#\" class=\"btn btn-primary\" data-dismiss=\"modal\">OK</a>'+                                    // 311\n              '</div>'+                                                                                               // 312\n            '</div>'+                                                                                                 // 313\n          '</div>' +                                                                                                  // 314\n        '</div>';                                                                                                     // 315\n        break;                                                                                                        // 316\n      case 'dropdown':                                                                                                // 317\n        template = '<div class=\"bootstrap-timepicker-widget dropdown-menu\">'+ templateContent +'</div>';              // 318\n        break;                                                                                                        // 319\n      }                                                                                                               // 320\n                                                                                                                      // 321\n      return template;                                                                                                // 322\n    },                                                                                                                // 323\n                                                                                                                      // 324\n    getTime: function() {                                                                                             // 325\n      if (this.hour === '') {                                                                                         // 326\n        return '';                                                                                                    // 327\n      }                                                                                                               // 328\n                                                                                                                      // 329\n      return this.hour + ':' + (this.minute.toString().length === 1 ? '0' + this.minute : this.minute) + (this.showSeconds ? ':' + (this.second.toString().length === 1 ? '0' + this.second : this.second) : '') + (this.showMeridian ? ' ' + this.meridian : '');\n    },                                                                                                                // 331\n                                                                                                                      // 332\n    hideWidget: function() {                                                                                          // 333\n      if (this.isOpen === false) {                                                                                    // 334\n        return;                                                                                                       // 335\n      }                                                                                                               // 336\n                                                                                                                      // 337\n      this.$element.trigger({                                                                                         // 338\n        'type': 'hide.timepicker',                                                                                    // 339\n        'time': {                                                                                                     // 340\n          'value': this.getTime(),                                                                                    // 341\n          'hours': this.hour,                                                                                         // 342\n          'minutes': this.minute,                                                                                     // 343\n          'seconds': this.second,                                                                                     // 344\n          'meridian': this.meridian                                                                                   // 345\n        }                                                                                                             // 346\n      });                                                                                                             // 347\n                                                                                                                      // 348\n      if (this.template === 'modal' && this.$widget.modal) {                                                          // 349\n        this.$widget.modal('hide');                                                                                   // 350\n      } else {                                                                                                        // 351\n        this.$widget.removeClass('open');                                                                             // 352\n      }                                                                                                               // 353\n                                                                                                                      // 354\n      $(document).off('mousedown.timepicker, touchend.timepicker');                                                   // 355\n                                                                                                                      // 356\n      this.isOpen = false;                                                                                            // 357\n      // show/hide approach taken by datepicker                                                                       // 358\n      this.$widget.detach();                                                                                          // 359\n    },                                                                                                                // 360\n                                                                                                                      // 361\n    highlightUnit: function() {                                                                                       // 362\n      this.position = this.getCursorPosition();                                                                       // 363\n      if (this.position >= 0 && this.position <= 2) {                                                                 // 364\n        this.highlightHour();                                                                                         // 365\n      } else if (this.position >= 3 && this.position <= 5) {                                                          // 366\n        this.highlightMinute();                                                                                       // 367\n      } else if (this.position >= 6 && this.position <= 8) {                                                          // 368\n        if (this.showSeconds) {                                                                                       // 369\n          this.highlightSecond();                                                                                     // 370\n        } else {                                                                                                      // 371\n          this.highlightMeridian();                                                                                   // 372\n        }                                                                                                             // 373\n      } else if (this.position >= 9 && this.position <= 11) {                                                         // 374\n        this.highlightMeridian();                                                                                     // 375\n      }                                                                                                               // 376\n    },                                                                                                                // 377\n                                                                                                                      // 378\n    highlightNextUnit: function() {                                                                                   // 379\n      switch (this.highlightedUnit) {                                                                                 // 380\n      case 'hour':                                                                                                    // 381\n        this.highlightMinute();                                                                                       // 382\n        break;                                                                                                        // 383\n      case 'minute':                                                                                                  // 384\n        if (this.showSeconds) {                                                                                       // 385\n          this.highlightSecond();                                                                                     // 386\n        } else if (this.showMeridian){                                                                                // 387\n          this.highlightMeridian();                                                                                   // 388\n        } else {                                                                                                      // 389\n          this.highlightHour();                                                                                       // 390\n        }                                                                                                             // 391\n        break;                                                                                                        // 392\n      case 'second':                                                                                                  // 393\n        if (this.showMeridian) {                                                                                      // 394\n          this.highlightMeridian();                                                                                   // 395\n        } else {                                                                                                      // 396\n          this.highlightHour();                                                                                       // 397\n        }                                                                                                             // 398\n        break;                                                                                                        // 399\n      case 'meridian':                                                                                                // 400\n        this.highlightHour();                                                                                         // 401\n        break;                                                                                                        // 402\n      }                                                                                                               // 403\n    },                                                                                                                // 404\n                                                                                                                      // 405\n    highlightPrevUnit: function() {                                                                                   // 406\n      switch (this.highlightedUnit) {                                                                                 // 407\n      case 'hour':                                                                                                    // 408\n        if(this.showMeridian){                                                                                        // 409\n          this.highlightMeridian();                                                                                   // 410\n        } else if (this.showSeconds) {                                                                                // 411\n          this.highlightSecond();                                                                                     // 412\n        } else {                                                                                                      // 413\n          this.highlightMinute();                                                                                     // 414\n        }                                                                                                             // 415\n        break;                                                                                                        // 416\n      case 'minute':                                                                                                  // 417\n        this.highlightHour();                                                                                         // 418\n        break;                                                                                                        // 419\n      case 'second':                                                                                                  // 420\n        this.highlightMinute();                                                                                       // 421\n        break;                                                                                                        // 422\n      case 'meridian':                                                                                                // 423\n        if (this.showSeconds) {                                                                                       // 424\n          this.highlightSecond();                                                                                     // 425\n        } else {                                                                                                      // 426\n          this.highlightMinute();                                                                                     // 427\n        }                                                                                                             // 428\n        break;                                                                                                        // 429\n      }                                                                                                               // 430\n    },                                                                                                                // 431\n                                                                                                                      // 432\n    highlightHour: function() {                                                                                       // 433\n      var $element = this.$element.get(0),                                                                            // 434\n          self = this;                                                                                                // 435\n                                                                                                                      // 436\n      this.highlightedUnit = 'hour';                                                                                  // 437\n                                                                                                                      // 438\n\t\t\tif ($element.setSelectionRange) {                                                                                  // 439\n\t\t\t\tsetTimeout(function() {                                                                                           // 440\n          if (self.hour < 10) {                                                                                       // 441\n            $element.setSelectionRange(0,1);                                                                          // 442\n          } else {                                                                                                    // 443\n            $element.setSelectionRange(0,2);                                                                          // 444\n          }                                                                                                           // 445\n\t\t\t\t}, 0);                                                                                                            // 446\n\t\t\t}                                                                                                                  // 447\n    },                                                                                                                // 448\n                                                                                                                      // 449\n    highlightMinute: function() {                                                                                     // 450\n      var $element = this.$element.get(0),                                                                            // 451\n          self = this;                                                                                                // 452\n                                                                                                                      // 453\n      this.highlightedUnit = 'minute';                                                                                // 454\n                                                                                                                      // 455\n\t\t\tif ($element.setSelectionRange) {                                                                                  // 456\n\t\t\t\tsetTimeout(function() {                                                                                           // 457\n          if (self.hour < 10) {                                                                                       // 458\n            $element.setSelectionRange(2,4);                                                                          // 459\n          } else {                                                                                                    // 460\n            $element.setSelectionRange(3,5);                                                                          // 461\n          }                                                                                                           // 462\n\t\t\t\t}, 0);                                                                                                            // 463\n\t\t\t}                                                                                                                  // 464\n    },                                                                                                                // 465\n                                                                                                                      // 466\n    highlightSecond: function() {                                                                                     // 467\n      var $element = this.$element.get(0),                                                                            // 468\n          self = this;                                                                                                // 469\n                                                                                                                      // 470\n      this.highlightedUnit = 'second';                                                                                // 471\n                                                                                                                      // 472\n\t\t\tif ($element.setSelectionRange) {                                                                                  // 473\n\t\t\t\tsetTimeout(function() {                                                                                           // 474\n          if (self.hour < 10) {                                                                                       // 475\n            $element.setSelectionRange(5,7);                                                                          // 476\n          } else {                                                                                                    // 477\n            $element.setSelectionRange(6,8);                                                                          // 478\n          }                                                                                                           // 479\n\t\t\t\t}, 0);                                                                                                            // 480\n\t\t\t}                                                                                                                  // 481\n    },                                                                                                                // 482\n                                                                                                                      // 483\n    highlightMeridian: function() {                                                                                   // 484\n      var $element = this.$element.get(0),                                                                            // 485\n          self = this;                                                                                                // 486\n                                                                                                                      // 487\n      this.highlightedUnit = 'meridian';                                                                              // 488\n                                                                                                                      // 489\n\t\t\tif ($element.setSelectionRange) {                                                                                  // 490\n\t\t\t\tif (this.showSeconds) {                                                                                           // 491\n\t\t\t\t\tsetTimeout(function() {                                                                                          // 492\n            if (self.hour < 10) {                                                                                     // 493\n              $element.setSelectionRange(8,10);                                                                       // 494\n            } else {                                                                                                  // 495\n              $element.setSelectionRange(9,11);                                                                       // 496\n            }                                                                                                         // 497\n\t\t\t\t\t}, 0);                                                                                                           // 498\n\t\t\t\t} else {                                                                                                          // 499\n\t\t\t\t\tsetTimeout(function() {                                                                                          // 500\n            if (self.hour < 10) {                                                                                     // 501\n              $element.setSelectionRange(5,7);                                                                        // 502\n            } else {                                                                                                  // 503\n              $element.setSelectionRange(6,8);                                                                        // 504\n            }                                                                                                         // 505\n\t\t\t\t\t}, 0);                                                                                                           // 506\n\t\t\t\t}                                                                                                                 // 507\n\t\t\t}                                                                                                                  // 508\n    },                                                                                                                // 509\n                                                                                                                      // 510\n    incrementHour: function() {                                                                                       // 511\n      if (this.showMeridian) {                                                                                        // 512\n        if (this.hour === 11) {                                                                                       // 513\n          this.hour++;                                                                                                // 514\n          return this.toggleMeridian();                                                                               // 515\n        } else if (this.hour === 12) {                                                                                // 516\n          this.hour = 0;                                                                                              // 517\n        }                                                                                                             // 518\n      }                                                                                                               // 519\n      if (this.hour === 23) {                                                                                         // 520\n        this.hour = 0;                                                                                                // 521\n                                                                                                                      // 522\n        return;                                                                                                       // 523\n      }                                                                                                               // 524\n      this.hour++;                                                                                                    // 525\n    },                                                                                                                // 526\n                                                                                                                      // 527\n    incrementMinute: function(step) {                                                                                 // 528\n      var newVal;                                                                                                     // 529\n                                                                                                                      // 530\n      if (step) {                                                                                                     // 531\n        newVal = this.minute + step;                                                                                  // 532\n      } else {                                                                                                        // 533\n        newVal = this.minute + this.minuteStep - (this.minute % this.minuteStep);                                     // 534\n      }                                                                                                               // 535\n                                                                                                                      // 536\n      if (newVal > 59) {                                                                                              // 537\n        this.incrementHour();                                                                                         // 538\n        this.minute = newVal - 60;                                                                                    // 539\n      } else {                                                                                                        // 540\n        this.minute = newVal;                                                                                         // 541\n      }                                                                                                               // 542\n    },                                                                                                                // 543\n                                                                                                                      // 544\n    incrementSecond: function() {                                                                                     // 545\n      var newVal = this.second + this.secondStep - (this.second % this.secondStep);                                   // 546\n                                                                                                                      // 547\n      if (newVal > 59) {                                                                                              // 548\n        this.incrementMinute(true);                                                                                   // 549\n        this.second = newVal - 60;                                                                                    // 550\n      } else {                                                                                                        // 551\n        this.second = newVal;                                                                                         // 552\n      }                                                                                                               // 553\n    },                                                                                                                // 554\n                                                                                                                      // 555\n    mousewheel: function(e) {                                                                                         // 556\n      if (this.disableMousewheel) {                                                                                   // 557\n        return;                                                                                                       // 558\n      }                                                                                                               // 559\n                                                                                                                      // 560\n      e.preventDefault();                                                                                             // 561\n      e.stopPropagation();                                                                                            // 562\n                                                                                                                      // 563\n      var delta = e.originalEvent.wheelDelta || -e.originalEvent.detail,                                              // 564\n          scrollTo = null;                                                                                            // 565\n                                                                                                                      // 566\n      if (e.type === 'mousewheel') {                                                                                  // 567\n        scrollTo = (e.originalEvent.wheelDelta * -1);                                                                 // 568\n      }                                                                                                               // 569\n      else if (e.type === 'DOMMouseScroll') {                                                                         // 570\n        scrollTo = 40 * e.originalEvent.detail;                                                                       // 571\n      }                                                                                                               // 572\n                                                                                                                      // 573\n      if (scrollTo) {                                                                                                 // 574\n        e.preventDefault();                                                                                           // 575\n        $(this).scrollTop(scrollTo + $(this).scrollTop());                                                            // 576\n      }                                                                                                               // 577\n                                                                                                                      // 578\n      switch (this.highlightedUnit) {                                                                                 // 579\n      case 'minute':                                                                                                  // 580\n        if (delta > 0) {                                                                                              // 581\n          this.incrementMinute();                                                                                     // 582\n        } else {                                                                                                      // 583\n          this.decrementMinute();                                                                                     // 584\n        }                                                                                                             // 585\n        this.highlightMinute();                                                                                       // 586\n        break;                                                                                                        // 587\n      case 'second':                                                                                                  // 588\n        if (delta > 0) {                                                                                              // 589\n          this.incrementSecond();                                                                                     // 590\n        } else {                                                                                                      // 591\n          this.decrementSecond();                                                                                     // 592\n        }                                                                                                             // 593\n        this.highlightSecond();                                                                                       // 594\n        break;                                                                                                        // 595\n      case 'meridian':                                                                                                // 596\n        this.toggleMeridian();                                                                                        // 597\n        this.highlightMeridian();                                                                                     // 598\n        break;                                                                                                        // 599\n      default:                                                                                                        // 600\n        if (delta > 0) {                                                                                              // 601\n          this.incrementHour();                                                                                       // 602\n        } else {                                                                                                      // 603\n          this.decrementHour();                                                                                       // 604\n        }                                                                                                             // 605\n        this.highlightHour();                                                                                         // 606\n        break;                                                                                                        // 607\n      }                                                                                                               // 608\n                                                                                                                      // 609\n      return false;                                                                                                   // 610\n    },                                                                                                                // 611\n                                                                                                                      // 612\n    // This method was adapted from bootstrap-datepicker.                                                             // 613\n    place : function() {                                                                                              // 614\n      if (this.isInline) {                                                                                            // 615\n        return;                                                                                                       // 616\n      }                                                                                                               // 617\n                                                                                                                      // 618\n      if (this.template === 'modal'){                                                                                 // 619\n        return ;                                                                                                      // 620\n      }                                                                                                               // 621\n                                                                                                                      // 622\n      var widgetWidth = this.$widget.outerWidth();                                                                    // 623\n      var widgetHeight = this.$widget.outerHeight();                                                                  // 624\n      var visualPadding = 10;                                                                                         // 625\n      var windowWidth = $(window).width();                                                                            // 626\n      var windowHeight = $(window).height();                                                                          // 627\n      var scrollTop = $(window).scrollTop();                                                                          // 628\n                                                                                                                      // 629\n      var zIndex = parseInt(this.$element.parents().filter(function() {}).first().css('z-index'), 10) + 10;           // 630\n      var offset = this.component ? this.component.parent().offset() : this.$element.offset();                        // 631\n      var height = this.component ? this.component.outerHeight(true) : this.$element.outerHeight(false);              // 632\n      var width = this.component ? this.component.outerWidth(true) : this.$element.outerWidth(false);                 // 633\n      var left = offset.left, top = offset.top;                                                                       // 634\n                                                                                                                      // 635\n      this.$widget.removeClass('timepicker-orient-top timepicker-orient-bottom timepicker-orient-right timepicker-orient-left');\n                                                                                                                      // 637\n      if (this.orientation.x !== 'auto') {                                                                            // 638\n        this.picker.addClass('datepicker-orient-' + this.orientation.x);                                              // 639\n        if (this.orientation.x === 'right') {                                                                         // 640\n          left -= widgetWidth - width;                                                                                // 641\n        }                                                                                                             // 642\n      } else{                                                                                                         // 643\n        // auto x orientation is best-placement: if it crosses a window edge, fudge it sideways                       // 644\n        // Default to left                                                                                            // 645\n        this.$widget.addClass('timepicker-orient-left');                                                              // 646\n        if (offset.left < 0) {                                                                                        // 647\n          left -= offset.left - visualPadding;                                                                        // 648\n        } else if (offset.left + widgetWidth > windowWidth) {                                                         // 649\n          left = windowWidth - widgetWidth - visualPadding;                                                           // 650\n        }                                                                                                             // 651\n      }                                                                                                               // 652\n      // auto y orientation is best-situation: top or bottom, no fudging, decision based on which shows more of the widget\n      var yorient = this.orientation.y, topOverflow, bottomOverflow;                                                  // 654\n      if (yorient === 'auto') {                                                                                       // 655\n        topOverflow = -scrollTop + offset.top - widgetHeight;                                                         // 656\n        bottomOverflow = scrollTop + windowHeight - (offset.top + height + widgetHeight);                             // 657\n        if (Math.max(topOverflow, bottomOverflow) === bottomOverflow) {                                               // 658\n          yorient = 'top';                                                                                            // 659\n        } else {                                                                                                      // 660\n          yorient = 'bottom';                                                                                         // 661\n        }                                                                                                             // 662\n      }                                                                                                               // 663\n      this.$widget.addClass('timepicker-orient-' + yorient);                                                          // 664\n      if (yorient === 'top'){                                                                                         // 665\n        top += height;                                                                                                // 666\n      } else{                                                                                                         // 667\n        top -= widgetHeight + parseInt(this.$widget.css('padding-top'), 10);                                          // 668\n      }                                                                                                               // 669\n                                                                                                                      // 670\n      this.$widget.css({                                                                                              // 671\n        top : top,                                                                                                    // 672\n        left : left,                                                                                                  // 673\n        zIndex : zIndex                                                                                               // 674\n      });                                                                                                             // 675\n    },                                                                                                                // 676\n                                                                                                                      // 677\n    remove: function() {                                                                                              // 678\n      $('document').off('.timepicker');                                                                               // 679\n      if (this.$widget) {                                                                                             // 680\n        this.$widget.remove();                                                                                        // 681\n      }                                                                                                               // 682\n      delete this.$element.data().timepicker;                                                                         // 683\n    },                                                                                                                // 684\n                                                                                                                      // 685\n    setDefaultTime: function(defaultTime) {                                                                           // 686\n      if (!this.$element.val()) {                                                                                     // 687\n        if (defaultTime === 'current') {                                                                              // 688\n          var dTime = new Date(),                                                                                     // 689\n            hours = dTime.getHours(),                                                                                 // 690\n            minutes = dTime.getMinutes(),                                                                             // 691\n            seconds = dTime.getSeconds(),                                                                             // 692\n            meridian = 'AM';                                                                                          // 693\n                                                                                                                      // 694\n          if (seconds !== 0) {                                                                                        // 695\n            seconds = Math.ceil(dTime.getSeconds() / this.secondStep) * this.secondStep;                              // 696\n            if (seconds === 60) {                                                                                     // 697\n              minutes += 1;                                                                                           // 698\n              seconds = 0;                                                                                            // 699\n            }                                                                                                         // 700\n          }                                                                                                           // 701\n                                                                                                                      // 702\n          if (minutes !== 0) {                                                                                        // 703\n            minutes = Math.ceil(dTime.getMinutes() / this.minuteStep) * this.minuteStep;                              // 704\n            if (minutes === 60) {                                                                                     // 705\n              hours += 1;                                                                                             // 706\n              minutes = 0;                                                                                            // 707\n            }                                                                                                         // 708\n          }                                                                                                           // 709\n                                                                                                                      // 710\n          if (this.showMeridian) {                                                                                    // 711\n            if (hours === 0) {                                                                                        // 712\n              hours = 12;                                                                                             // 713\n            } else if (hours >= 12) {                                                                                 // 714\n              if (hours > 12) {                                                                                       // 715\n                hours = hours - 12;                                                                                   // 716\n              }                                                                                                       // 717\n              meridian = 'PM';                                                                                        // 718\n            } else {                                                                                                  // 719\n              meridian = 'AM';                                                                                        // 720\n            }                                                                                                         // 721\n          }                                                                                                           // 722\n                                                                                                                      // 723\n          this.hour = hours;                                                                                          // 724\n          this.minute = minutes;                                                                                      // 725\n          this.second = seconds;                                                                                      // 726\n          this.meridian = meridian;                                                                                   // 727\n                                                                                                                      // 728\n          this.update();                                                                                              // 729\n                                                                                                                      // 730\n        } else if (defaultTime === false) {                                                                           // 731\n          this.hour = 0;                                                                                              // 732\n          this.minute = 0;                                                                                            // 733\n          this.second = 0;                                                                                            // 734\n          this.meridian = 'AM';                                                                                       // 735\n        } else {                                                                                                      // 736\n          this.setTime(defaultTime);                                                                                  // 737\n        }                                                                                                             // 738\n      } else {                                                                                                        // 739\n        this.updateFromElementVal();                                                                                  // 740\n      }                                                                                                               // 741\n    },                                                                                                                // 742\n                                                                                                                      // 743\n    setTime: function(time, ignoreWidget) {                                                                           // 744\n      if (!time) {                                                                                                    // 745\n        this.clear();                                                                                                 // 746\n        return;                                                                                                       // 747\n      }                                                                                                               // 748\n                                                                                                                      // 749\n      var timeArray,                                                                                                  // 750\n          hour,                                                                                                       // 751\n          minute,                                                                                                     // 752\n          second,                                                                                                     // 753\n          meridian;                                                                                                   // 754\n                                                                                                                      // 755\n      if (typeof time === 'object' && time.getMonth){                                                                 // 756\n        // this is a date object                                                                                      // 757\n        hour    = time.getHours();                                                                                    // 758\n        minute  = time.getMinutes();                                                                                  // 759\n        second  = time.getSeconds();                                                                                  // 760\n                                                                                                                      // 761\n        if (this.showMeridian){                                                                                       // 762\n          meridian = 'AM';                                                                                            // 763\n          if (hour > 12){                                                                                             // 764\n            meridian = 'PM';                                                                                          // 765\n            hour = hour % 12;                                                                                         // 766\n          }                                                                                                           // 767\n                                                                                                                      // 768\n          if (hour === 12){                                                                                           // 769\n            meridian = 'PM';                                                                                          // 770\n          }                                                                                                           // 771\n        }                                                                                                             // 772\n      } else {                                                                                                        // 773\n        if (time.match(/p/i) !== null) {                                                                              // 774\n          meridian = 'PM';                                                                                            // 775\n        } else {                                                                                                      // 776\n          meridian = 'AM';                                                                                            // 777\n        }                                                                                                             // 778\n                                                                                                                      // 779\n        time = time.replace(/[^0-9\\:]/g, '');                                                                         // 780\n                                                                                                                      // 781\n        timeArray = time.split(':');                                                                                  // 782\n                                                                                                                      // 783\n        hour = timeArray[0] ? timeArray[0].toString() : timeArray.toString();                                         // 784\n        minute = timeArray[1] ? timeArray[1].toString() : '';                                                         // 785\n        second = timeArray[2] ? timeArray[2].toString() : '';                                                         // 786\n                                                                                                                      // 787\n        // idiot proofing                                                                                             // 788\n        if (hour.length > 4) {                                                                                        // 789\n          second = hour.substr(4, 2);                                                                                 // 790\n        }                                                                                                             // 791\n        if (hour.length > 2) {                                                                                        // 792\n          minute = hour.substr(2, 2);                                                                                 // 793\n          hour = hour.substr(0, 2);                                                                                   // 794\n        }                                                                                                             // 795\n        if (minute.length > 2) {                                                                                      // 796\n          second = minute.substr(2, 2);                                                                               // 797\n          minute = minute.substr(0, 2);                                                                               // 798\n        }                                                                                                             // 799\n        if (second.length > 2) {                                                                                      // 800\n          second = second.substr(2, 2);                                                                               // 801\n        }                                                                                                             // 802\n                                                                                                                      // 803\n        hour = parseInt(hour, 10);                                                                                    // 804\n        minute = parseInt(minute, 10);                                                                                // 805\n        second = parseInt(second, 10);                                                                                // 806\n                                                                                                                      // 807\n        if (isNaN(hour)) {                                                                                            // 808\n          hour = 0;                                                                                                   // 809\n        }                                                                                                             // 810\n        if (isNaN(minute)) {                                                                                          // 811\n          minute = 0;                                                                                                 // 812\n        }                                                                                                             // 813\n        if (isNaN(second)) {                                                                                          // 814\n          second = 0;                                                                                                 // 815\n        }                                                                                                             // 816\n                                                                                                                      // 817\n        if (this.showMeridian) {                                                                                      // 818\n          if (hour < 1) {                                                                                             // 819\n            hour = 1;                                                                                                 // 820\n          } else if (hour > 12) {                                                                                     // 821\n            hour = 12;                                                                                                // 822\n          }                                                                                                           // 823\n        } else {                                                                                                      // 824\n          if (hour >= 24) {                                                                                           // 825\n            hour = 23;                                                                                                // 826\n          } else if (hour < 0) {                                                                                      // 827\n            hour = 0;                                                                                                 // 828\n          }                                                                                                           // 829\n          if (hour < 13 && meridian === 'PM') {                                                                       // 830\n            hour = hour + 12;                                                                                         // 831\n          }                                                                                                           // 832\n        }                                                                                                             // 833\n                                                                                                                      // 834\n        if (minute < 0) {                                                                                             // 835\n          minute = 0;                                                                                                 // 836\n        } else if (minute >= 60) {                                                                                    // 837\n          minute = 59;                                                                                                // 838\n        }                                                                                                             // 839\n                                                                                                                      // 840\n        if (this.showSeconds) {                                                                                       // 841\n          if (isNaN(second)) {                                                                                        // 842\n            second = 0;                                                                                               // 843\n          } else if (second < 0) {                                                                                    // 844\n            second = 0;                                                                                               // 845\n          } else if (second >= 60) {                                                                                  // 846\n            second = 59;                                                                                              // 847\n          }                                                                                                           // 848\n        }                                                                                                             // 849\n      }                                                                                                               // 850\n                                                                                                                      // 851\n      this.hour = hour;                                                                                               // 852\n      this.minute = minute;                                                                                           // 853\n      this.second = second;                                                                                           // 854\n      this.meridian = meridian;                                                                                       // 855\n                                                                                                                      // 856\n      this.update(ignoreWidget);                                                                                      // 857\n    },                                                                                                                // 858\n                                                                                                                      // 859\n    showWidget: function() {                                                                                          // 860\n      if (this.isOpen) {                                                                                              // 861\n        return;                                                                                                       // 862\n      }                                                                                                               // 863\n                                                                                                                      // 864\n      if (this.$element.is(':disabled')) {                                                                            // 865\n        return;                                                                                                       // 866\n      }                                                                                                               // 867\n                                                                                                                      // 868\n      // show/hide approach taken by datepicker                                                                       // 869\n      this.$widget.appendTo(this.appendWidgetTo);                                                                     // 870\n      var self = this;                                                                                                // 871\n      $(document).on('mousedown.timepicker, touchend.timepicker', function (e) {                                      // 872\n        // This condition was inspired by bootstrap-datepicker.                                                       // 873\n        // The element the timepicker is invoked on is the input but it has a sibling for addon/button.               // 874\n        if (!(self.$element.parent().find(e.target).length ||                                                         // 875\n            self.$widget.is(e.target) ||                                                                              // 876\n            self.$widget.find(e.target).length)) {                                                                    // 877\n          self.hideWidget();                                                                                          // 878\n        }                                                                                                             // 879\n      });                                                                                                             // 880\n                                                                                                                      // 881\n      this.$element.trigger({                                                                                         // 882\n        'type': 'show.timepicker',                                                                                    // 883\n        'time': {                                                                                                     // 884\n          'value': this.getTime(),                                                                                    // 885\n          'hours': this.hour,                                                                                         // 886\n          'minutes': this.minute,                                                                                     // 887\n          'seconds': this.second,                                                                                     // 888\n          'meridian': this.meridian                                                                                   // 889\n        }                                                                                                             // 890\n      });                                                                                                             // 891\n                                                                                                                      // 892\n      this.place();                                                                                                   // 893\n                                                                                                                      // 894\n      //this.$widget.popover();                                                                                       // 895\n                                                                                                                      // 896\n      if (this.disableFocus) {                                                                                        // 897\n        this.$element.blur();                                                                                         // 898\n      }                                                                                                               // 899\n                                                                                                                      // 900\n      // widget shouldn't be empty on open                                                                            // 901\n      if (this.hour === '') {                                                                                         // 902\n        if (this.defaultTime) {                                                                                       // 903\n          this.setDefaultTime(this.defaultTime);                                                                      // 904\n        } else {                                                                                                      // 905\n          this.setTime('0:0:0');                                                                                      // 906\n        }                                                                                                             // 907\n      }                                                                                                               // 908\n                                                                                                                      // 909\n      if (this.template === 'modal' && this.$widget.modal) {                                                          // 910\n        this.$widget.modal('show').on('hidden', $.proxy(this.hideWidget, this));                                      // 911\n      } else {                                                                                                        // 912\n        if (this.isOpen === false) {                                                                                  // 913\n          this.$widget.addClass('open');                                                                              // 914\n        }                                                                                                             // 915\n      }                                                                                                               // 916\n                                                                                                                      // 917\n      this.isOpen = true;                                                                                             // 918\n    },                                                                                                                // 919\n                                                                                                                      // 920\n    toggleMeridian: function() {                                                                                      // 921\n      this.meridian = this.meridian === 'AM' ? 'PM' : 'AM';                                                           // 922\n    },                                                                                                                // 923\n                                                                                                                      // 924\n    update: function(ignoreWidget) {                                                                                  // 925\n      this.updateElement();                                                                                           // 926\n      if (!ignoreWidget) {                                                                                            // 927\n        this.updateWidget();                                                                                          // 928\n      }                                                                                                               // 929\n                                                                                                                      // 930\n      this.$element.trigger({                                                                                         // 931\n        'type': 'changeTime.timepicker',                                                                              // 932\n        'time': {                                                                                                     // 933\n          'value': this.getTime(),                                                                                    // 934\n          'hours': this.hour,                                                                                         // 935\n          'minutes': this.minute,                                                                                     // 936\n          'seconds': this.second,                                                                                     // 937\n          'meridian': this.meridian                                                                                   // 938\n        }                                                                                                             // 939\n      });                                                                                                             // 940\n    },                                                                                                                // 941\n                                                                                                                      // 942\n    updateElement: function() {                                                                                       // 943\n      this.$element.val(this.getTime()).change();                                                                     // 944\n    },                                                                                                                // 945\n                                                                                                                      // 946\n    updateFromElementVal: function() {                                                                                // 947\n      this.setTime(this.$element.val());                                                                              // 948\n    },                                                                                                                // 949\n                                                                                                                      // 950\n    updateWidget: function() {                                                                                        // 951\n      if (this.$widget === false) {                                                                                   // 952\n        return;                                                                                                       // 953\n      }                                                                                                               // 954\n                                                                                                                      // 955\n      var hour = this.hour,                                                                                           // 956\n          minute = this.minute.toString().length === 1 ? '0' + this.minute : this.minute,                             // 957\n          second = this.second.toString().length === 1 ? '0' + this.second : this.second;                             // 958\n                                                                                                                      // 959\n      if (this.showInputs) {                                                                                          // 960\n        this.$widget.find('input.bootstrap-timepicker-hour').val(hour);                                               // 961\n        this.$widget.find('input.bootstrap-timepicker-minute').val(minute);                                           // 962\n                                                                                                                      // 963\n        if (this.showSeconds) {                                                                                       // 964\n          this.$widget.find('input.bootstrap-timepicker-second').val(second);                                         // 965\n        }                                                                                                             // 966\n        if (this.showMeridian) {                                                                                      // 967\n          this.$widget.find('input.bootstrap-timepicker-meridian').val(this.meridian);                                // 968\n        }                                                                                                             // 969\n      } else {                                                                                                        // 970\n        this.$widget.find('span.bootstrap-timepicker-hour').text(hour);                                               // 971\n        this.$widget.find('span.bootstrap-timepicker-minute').text(minute);                                           // 972\n                                                                                                                      // 973\n        if (this.showSeconds) {                                                                                       // 974\n          this.$widget.find('span.bootstrap-timepicker-second').text(second);                                         // 975\n        }                                                                                                             // 976\n        if (this.showMeridian) {                                                                                      // 977\n          this.$widget.find('span.bootstrap-timepicker-meridian').text(this.meridian);                                // 978\n        }                                                                                                             // 979\n      }                                                                                                               // 980\n    },                                                                                                                // 981\n                                                                                                                      // 982\n    updateFromWidgetInputs: function() {                                                                              // 983\n      if (this.$widget === false) {                                                                                   // 984\n        return;                                                                                                       // 985\n      }                                                                                                               // 986\n                                                                                                                      // 987\n      var t = this.$widget.find('input.bootstrap-timepicker-hour').val() + ':' +                                      // 988\n              this.$widget.find('input.bootstrap-timepicker-minute').val() +                                          // 989\n              (this.showSeconds ? ':' + this.$widget.find('input.bootstrap-timepicker-second').val() : '') +          // 990\n              (this.showMeridian ? this.$widget.find('input.bootstrap-timepicker-meridian').val() : '')               // 991\n      ;                                                                                                               // 992\n                                                                                                                      // 993\n      this.setTime(t, true);                                                                                          // 994\n    },                                                                                                                // 995\n                                                                                                                      // 996\n    widgetClick: function(e) {                                                                                        // 997\n      e.stopPropagation();                                                                                            // 998\n      e.preventDefault();                                                                                             // 999\n                                                                                                                      // 1000\n      var $input = $(e.target),                                                                                       // 1001\n          action = $input.closest('a').data('action');                                                                // 1002\n                                                                                                                      // 1003\n      if (action) {                                                                                                   // 1004\n        this[action]();                                                                                               // 1005\n      }                                                                                                               // 1006\n      this.update();                                                                                                  // 1007\n                                                                                                                      // 1008\n      if ($input.is('input')) {                                                                                       // 1009\n        $input.get(0).setSelectionRange(0,2);                                                                         // 1010\n      }                                                                                                               // 1011\n    },                                                                                                                // 1012\n                                                                                                                      // 1013\n    widgetKeydown: function(e) {                                                                                      // 1014\n      var $input = $(e.target),                                                                                       // 1015\n          name = $input.data('name');                                                                                 // 1016\n                                                                                                                      // 1017\n      switch (e.keyCode) {                                                                                            // 1018\n      case 9: //tab                                                                                                   // 1019\n        if ((this.showMeridian && name === 'meridian') || (this.showSeconds && name === 'second') || (!this.showMeridian && !this.showSeconds && name === 'minute')) {\n          return this.hideWidget();                                                                                   // 1021\n        }                                                                                                             // 1022\n        break;                                                                                                        // 1023\n      case 27: // escape                                                                                              // 1024\n        this.hideWidget();                                                                                            // 1025\n        break;                                                                                                        // 1026\n      case 38: // up arrow                                                                                            // 1027\n        e.preventDefault();                                                                                           // 1028\n        switch (name) {                                                                                               // 1029\n        case 'hour':                                                                                                  // 1030\n          this.incrementHour();                                                                                       // 1031\n          break;                                                                                                      // 1032\n        case 'minute':                                                                                                // 1033\n          this.incrementMinute();                                                                                     // 1034\n          break;                                                                                                      // 1035\n        case 'second':                                                                                                // 1036\n          this.incrementSecond();                                                                                     // 1037\n          break;                                                                                                      // 1038\n        case 'meridian':                                                                                              // 1039\n          this.toggleMeridian();                                                                                      // 1040\n          break;                                                                                                      // 1041\n        }                                                                                                             // 1042\n        this.setTime(this.getTime());                                                                                 // 1043\n        $input.get(0).setSelectionRange(0,2);                                                                         // 1044\n        break;                                                                                                        // 1045\n      case 40: // down arrow                                                                                          // 1046\n        e.preventDefault();                                                                                           // 1047\n        switch (name) {                                                                                               // 1048\n        case 'hour':                                                                                                  // 1049\n          this.decrementHour();                                                                                       // 1050\n          break;                                                                                                      // 1051\n        case 'minute':                                                                                                // 1052\n          this.decrementMinute();                                                                                     // 1053\n          break;                                                                                                      // 1054\n        case 'second':                                                                                                // 1055\n          this.decrementSecond();                                                                                     // 1056\n          break;                                                                                                      // 1057\n        case 'meridian':                                                                                              // 1058\n          this.toggleMeridian();                                                                                      // 1059\n          break;                                                                                                      // 1060\n        }                                                                                                             // 1061\n        this.setTime(this.getTime());                                                                                 // 1062\n        $input.get(0).setSelectionRange(0,2);                                                                         // 1063\n        break;                                                                                                        // 1064\n      }                                                                                                               // 1065\n    },                                                                                                                // 1066\n                                                                                                                      // 1067\n    widgetKeyup: function(e) {                                                                                        // 1068\n      if ((e.keyCode === 65) || (e.keyCode === 77) || (e.keyCode === 80) || (e.keyCode === 46) || (e.keyCode === 8) || (e.keyCode >= 46 && e.keyCode <= 57)) {\n        this.updateFromWidgetInputs();                                                                                // 1070\n      }                                                                                                               // 1071\n    }                                                                                                                 // 1072\n  };                                                                                                                  // 1073\n                                                                                                                      // 1074\n  //TIMEPICKER PLUGIN DEFINITION                                                                                      // 1075\n  $.fn.timepicker = function(option) {                                                                                // 1076\n    var args = Array.apply(null, arguments);                                                                          // 1077\n    args.shift();                                                                                                     // 1078\n    return this.each(function() {                                                                                     // 1079\n      var $this = $(this),                                                                                            // 1080\n        data = $this.data('timepicker'),                                                                              // 1081\n        options = typeof option === 'object' && option;                                                               // 1082\n                                                                                                                      // 1083\n      if (!data) {                                                                                                    // 1084\n        $this.data('timepicker', (data = new Timepicker(this, $.extend({}, $.fn.timepicker.defaults, options, $(this).data()))));\n      }                                                                                                               // 1086\n                                                                                                                      // 1087\n      if (typeof option === 'string') {                                                                               // 1088\n        data[option].apply(data, args);                                                                               // 1089\n      }                                                                                                               // 1090\n    });                                                                                                               // 1091\n  };                                                                                                                  // 1092\n                                                                                                                      // 1093\n  $.fn.timepicker.defaults = {                                                                                        // 1094\n    defaultTime: 'current',                                                                                           // 1095\n    disableFocus: false,                                                                                              // 1096\n    disableMousewheel: false,                                                                                         // 1097\n    isOpen: false,                                                                                                    // 1098\n    minuteStep: 15,                                                                                                   // 1099\n    modalBackdrop: false,                                                                                             // 1100\n    orientation: { x: 'auto', y: 'auto'},                                                                             // 1101\n    secondStep: 15,                                                                                                   // 1102\n    showSeconds: false,                                                                                               // 1103\n    showInputs: true,                                                                                                 // 1104\n    showMeridian: true,                                                                                               // 1105\n    template: 'dropdown',                                                                                             // 1106\n    appendWidgetTo: 'body',                                                                                           // 1107\n    showWidgetOnAddonClick: true                                                                                      // 1108\n  };                                                                                                                  // 1109\n                                                                                                                      // 1110\n  $.fn.timepicker.Constructor = Timepicker;                                                                           // 1111\n                                                                                                                      // 1112\n})(jQuery, window, document);                                                                                         // 1113\n                                                                                                                      // 1114\n////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n\n}).call(this);\n\n\n\n\n\n\n(function () {\n\n////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n//                                                                                                                    //\n// packages/jplack:bootstrap-timepicker/timepicker.js                                                                 //\n//                                                                                                                    //\n////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n                                                                                                                      //\nif (Meteor.isClient) {                                                                                                // 1\n  Template.onRendered(function () {                                                                                   // 2\n    this.$('.timepicker').timepicker();                                                                               // 3\n    this.$('.timepicker-precise').timepicker({                                                                        // 4\n      minuteStep: 1                                                                                                   // 5\n    });                                                                                                               // 6\n    this.$('.timepicker-seconds').timepicker({                                                                        // 7\n      minuteStep: 1,                                                                                                  // 8\n      secondStep: 1,                                                                                                  // 9\n      showSeconds: true                                                                                               // 10\n    });                                                                                                               // 11\n  });                                                                                                                 // 12\n}                                                                                                                     // 13\n////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n\n}).call(this);\n"]}